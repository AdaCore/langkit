grammar None_grammar {
    main_rule <- Literal(@Number)

}

@abstract class FooNode {

    @export fun prop (): Integer

    fun prop (): Integer
}

class Literal : FooNode {
    var : LogicVar

    fun a (): Entity[FooNode]

    fun node (): Entity[Literal] = self.as_entity

    fun b (): LogicEquation = %eq(self.var, self.a, conv_prop=BareLiteral.node)

    @export fun public_pro (): Entity[Literal] = {
        val _ = self.b;

        self.as_bare_entity
    }

    fun a (): Entity[FooNode]
}
