class FooNode is

    public fun prop (empty1 : Boolean, empty2 : Boolean): Boolean =
        let
            arg1 = if empty1 then _ else self.children_env
            arg2 = if empty2 then _ else self.children_env
        in
            <is_visible_from(<Var arg1>, <Var arg2>)>
        end

end

