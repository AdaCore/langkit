class Example : FooNode is
    field var1 : LogicVar
    field var2 : LogicVar

    fun predicate (n : Entity[FooNode], flag1 : Boolean = false, flag2 : Boolean = true): Boolean = (flag1 and (n.is_null)) and flag2

    public fun prop (): Entity[Example] =
        let
            _ = <Predicate on Example.predicate>
        in
            self.as_bare_entity
        end

