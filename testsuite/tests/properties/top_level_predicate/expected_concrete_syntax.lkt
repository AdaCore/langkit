grammar None_grammar {
    main_rule <- Example("example")

}

class FooNode {

    fun sophisticated_predicate (): Boolean = true
}

class Example : FooNode {
    var1 : LogicVar

    @export fun prop (): Entity[Example] = {
        val _ = %predicate(BareFooNode.sophisticated_predicate, self.var1);

        self.as_bare_entity
    }
}
