grammar None_grammar {
    main_rule <- Literal(@Number)

}

@root_node class FooNode {

    @export fun prop (): Int

    fun prop (): Int
}

class Literal : FooNode {
    var : LogicVar

    fun a (): FooNode

    fun b (): LogicEquation = %eq(node.var, node.a)

    @export fun public_prop (): Literal = {
        val _ = node.b;

        node.as_bare_entity
    }

    fun a (): FooNode
}
