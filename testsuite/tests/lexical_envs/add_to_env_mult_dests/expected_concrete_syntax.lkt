grammar None_grammar {
    main_rule <- elem
    scope <- Scope(id @LBrace list*(elem) @RBrace)
    id <- Id(@Identifier)
    elem <- or(scope | id)

}

class FooNode {
}

"""
List of FooNode.

This list node can contain one of the following nodes:

* ${node_name(T.Id)}
* ${node_name(T.Scope)}
"""
class ASTList[FooNode] : FooNodeBaseList {
}

class Id : FooNode {

    public fun get_scope (): Entity[FooNode] = self.children_env.get("Scope", only_first=true)
}

class Scope : FooNode {
    parse field name : Id
    parse field content : ASTList[FooNode]
}
