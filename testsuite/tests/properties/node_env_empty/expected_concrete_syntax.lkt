grammar None_grammar {
    decl <- Decl("def" list*(name))
    main_rule <- list*(decl)
    name <- Name(@Identifier)

}

@root_node class FooNode {
}

class Decl : FooNode {
    @parse_field names : ASTList[Name]

    @export fun lookup (n : Name): FooNode =
    self.node_env.get(n.symbol, only_first=true)
}

class Name : FooNode {
}
