import lexer_example

@with_lexer(foo_lexer)
grammar foo_grammar {
    @main_rule main_rule <- BarNode("example")

}

@abstract class FooNode implements Node[FooNode] {
}

class BarNode : FooNode {

    @external fun foo(i: Int): BigInt

    @export fun prop(): BigInt = {
        val _ = node.foo(1);

        node.foo(2)
    }
}
