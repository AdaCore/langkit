grammar foo_grammar {
    @main_rule main_rule <- Literal(@number)

}

@root_node class FooNode {

    @export fun prop (): Int

    fun prop (): Int
}

class Literal : FooNode {
    var : LogicVar

    fun a (): FooNode

    fun node (): Literal = node.as_entity

    fun b (): LogicEquation = %eq(node.var, node.a, conv_prop=BareLiteral.node)

    @export fun public_pro (): Literal = {
        val _ = node.b;

        node.as_bare_entity
    }

    fun a (): FooNode
}
