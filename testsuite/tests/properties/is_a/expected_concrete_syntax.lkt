grammar None_grammar {
    main_rule <- list*(Def(@Def Example(@Example)))

}

class FooNode {

    fun null_node (): FooNode = null.to[FooNode]

    @export fun is_null_a_def (): Boolean = self.null_node isa Def
}

class Def : FooNode {
    @parse_field example : Example
}

class Example : FooNode {
}
