import lexer_example
grammar foo_grammar {
    @main_rule main_rule <- Literal(@number)

}

@root_node class FooNode {

    @export fun prop (): Int

    fun prop (): Int
}

@token_node class Literal : FooNode {
    var : LogicVar

    fun a (): FooNode

    fun b (): LogicEquation = %eq(node.var, node.a())

    @export fun public_prop (): Literal = {
        val _ = node.b();

        node.as_bare_entity
    }

    fun a (): FooNode
}
