grammar None_grammar {
    main_rule <- Example("example")

}

@abstract class FooNode {
    v : LogicVar
}

@abstract class SomeNode : FooNode {

    fun test_prop (): Boolean
}

class Example : SomeNode {

    fun test_prop (): Boolean = true

    @export fun solve (): Boolean = (
        %domain(self.v, [null.to[Entity[FooNode]]])
    ) and (
        %predicate(BareSomeNode.test_prop, self.v)
    ).solve
}
