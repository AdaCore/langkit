import lexer_example
grammar foo_grammar {
    @main_rule main_rule <- list+(Example("example"))

}

@root_node class FooNode {

    @export fun increment (kv : KV): KV = kv.update(value=kv.value + BigInt(1))
}

@token_node class Example : FooNode {
}

struct KV {
    key : String
    value : BigInt
}
