import lexer_example
grammar foo_grammar {
    @main_rule main_rule <- tuple
    tuple <- Tuple(Name(@identifier) "(" list*(tuple) ")")

}

@abstract class FooNode implements Node {
}

class Name : FooNode implements SymbolNode {
}

class Tuple : FooNode {
    @parse_field id : Name
    @parse_field nodes : ASTList[Tuple]
}
