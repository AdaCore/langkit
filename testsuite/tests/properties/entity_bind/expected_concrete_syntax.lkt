grammar None_grammar {
    main_rule <- Literal(@Number)

}

class FooNode {

    public fun prop (): Integer

    fun prop (): Integer
}

class Literal : FooNode {
    @field var : LogicVar

    fun a (): Entity[FooNode]

    fun b (): LogicEquation = <Bind>

    public fun public_prop (): Entity[Literal] = {
        val _ = self.b

        self.as_bare_entity
    }

    fun a (): Entity[FooNode]
}
