# Check that cycling derivations (AbstractA derives AbstractB, which derives
# AbstractA) are correctly rejected.
#
from lexer_example import foo_lexer

@with_lexer(foo_lexer)
grammar foo_grammar {
    @main_rule main_rule <- Example("example")
}

class FooNode implements Node[FooNode] {
}

@abstract
class AbstractA: AbstractB {
}

@abstract
class AbstractB: AbstractA {
}

class Example: AbstractA implements TokenNode {
}
