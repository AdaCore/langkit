grammar None_grammar is
    main_rule <- Literal(@Number)

end

class FooNode is

    public fun prop (): Integer

    fun prop (): Integer
end

class Literal : FooNode is
    field var : LogicVar

    fun a (): Entity[FooNode]

    fun b (): LogicEquation = <Bind>

    public fun public_prop (): Entity[Literal] =
        let
            _ = self.b
        in
            self.as_bare_entity
        end


    fun a (): Entity[FooNode]
end
