grammar None_grammar {
    decl <- Decl(@Def list*(name))
    main_rule <- list*(decl)
    name <- Name(@Identifier)

}

class FooNode {
}

class Decl : FooNode {
    @parse_field names : ASTList[Name]

    @export fun lookup (n : Entity[Name]): Entity[FooNode] =
    entity.node_env.get(n.symbol, only_first=true)
}

class Name : FooNode {
}
