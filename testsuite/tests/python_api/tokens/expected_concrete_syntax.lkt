import lexer_example
@with_lexer(foo_lexer)
grammar foo_grammar {
    @main_rule main_rule <- element
    element <- or(sequence | atom)
    sequence <- pick("(" Sequence*(element) ")")
    atom <- Atom(@identifier)

}

@abstract @has_abstract_list class FooNode implements Node[FooNode] {
}

class Atom : FooNode implements TokenNode {
}

class Sequence : ASTList[FooNode, FooNode] {
}
