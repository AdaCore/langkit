grammar None_grammar is
    main_rule <- list*(or(
        | DefNode(
            @Def identifier @Equal number @Semicolon
        )
        | skip(ErrorDef)
    ))
    identifier <- Identifier(@Identifier)
    number <- Number(@Number)

end
