import lexer_example
grammar foo_grammar {
    @main_rule main_rule <- list+(Def("def" Example("example")))

}

@root_node class FooNode {

    fun null_node (): FooNode = null

    @export fun is_null_a_def (): Bool = node.null_node() is Def

    @export fun is_null_a_def_or_example (): Bool =
    node.null_node() is Def | Example
}

class Def : FooNode {
    @parse_field example : Example
}

@token_node class Example : FooNode {
}
