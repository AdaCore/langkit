grammar None_grammar {
    main_rule <- elem
    scope <- Scope(id "{" list*(elem) "}")
    id <- Id(@Identifier)
    elem <- or(scope | id)

}

class FooNode {
}

class Id : FooNode {

    @export fun get_scope (): Entity[FooNode] =
    self.children_env.get("Scope", only_first=true)
}

class Scope : FooNode {
    @parse_field name : Id
    @parse_field content : ASTList[FooNode]
}
