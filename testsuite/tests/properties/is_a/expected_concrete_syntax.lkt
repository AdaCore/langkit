grammar foo_grammar {
    @main_rule main_rule <- list+(Def("def" Example("example")))

}

@root_node class FooNode {

    fun null_node (): FooNode = null

    @export fun is_null_a_def (): Boolean = node.null_node isa Def

    @export fun is_null_a_def_or_example (): Boolean =
    node.null_node isa Def | Example
}

class Def : FooNode {
    @parse_field example : Example
}

class Example : FooNode {
}
