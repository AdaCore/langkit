# Combine never_absent/maybe_absent with pre/post tokens reconciliation

import common_field_combine

@with_lexer(foo_lexer)
@with_unparsers
grammar foo_grammar {
    @main_rule main_rule <- or(
        | Assoc("(" name "=" name ")")
        | Assoc("(" ?(name "=") name ")")
    )
    name <- Name(@Identifier)
}
