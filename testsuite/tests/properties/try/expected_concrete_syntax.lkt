grammar None_grammar {
    main_rule <- Example(@Example)

}

class FooNode {
}

class Example : FooNode {

    fun failing_property (): Boolean = raise PropertyError()

    @export fun failsafe_property (): Boolean =
    try self.failing_property or (false)

    @export fun failsafe_property_2 (): Boolean =
    try self.failing_property
}
