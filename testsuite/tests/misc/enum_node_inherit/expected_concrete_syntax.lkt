grammar FooGrammar {
    @main_rule main_rule <- HasExample("example")

}

@root_node class FooNode {
}

@abstract class EnumNode : FooNode {

    @export fun prop (): Boolean = true
}

@abstract @qualifier class HasExample : EnumNode {

    ## Return whether this is an instance of HasExamplePresent
    @export fun as_bool (): Boolean
}
