grammar None_grammar {
    main_rule <- list*(def_rule)
    def_rule <- Def(name ?"+" name)
    name <- Name(@Identifier)

}

@root_node class FooNode {
}

class Def : FooNode {
    @parse_field name : Name
    @parse_field ref : Name
}

class Name : FooNode {

    fun sym (): SymbolType = node.symbol

    fun resolve (): FooNode = node.parent.node_env.get(node.sym)?(0)
}

struct EnvAssoc {
    key : SymbolType
    value : FooNode
    dest_env : LexicalEnv
    metadata : Metadata
}
